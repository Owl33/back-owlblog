{
  "version": 2,

  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "https://back-owlblog.vercel.app/v1/$1",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
